Params.set_params(int_precision=64, f=32, k=64)
NUM_PARTIES = 3

SGD_ITERS = 2
data_size = 569
dim = 30
BATCH_SIZE = 5

X = s_fix_mat.read_input(data_size, dim, 0)
y = s_fix_mat.read_input(data_size, 1, 0)

w = s_fix_mat(dim, 1)
alpha_B = c_fix(0.002)

XB = s_fix_mat(BATCH_SIZE, dim)
yB = s_fix_mat(BATCH_SIZE, 1)

for i in range(SGD_ITERS):
    batch_low = i * BATCH_SIZE
    batch_high = (i + 1) * BATCH_SIZE

    for j in range(BATCH_SIZE):
        for d in range(dim):
            XB[j][d] = X[batch_low + j][d]
        yB[j][0] = y[batch_low + j][0]

    w_ret = matmul(XB, w)
    reveal_all(w_ret, "w_ret")
    w_sigmoid = sigmoid(w_ret)
    reveal_all(w_sigmoid, "w_sigmoid")
    w_sub = matsub(w_sigmoid, yB)
    reveal_all(w_sub, "w_sub")
    reveal_all(XB[0], "XB[0]")
    reveal_all(XB[1], "XB[1]")
    XB_T = transpose(XB)
    reveal_all(XB_T[0], "XB_T[0]")
    reveal_all(XB_T[1], "XB_T[1]")
    v1 = XB_T[0][0] * w_sub[0][0]
    v2 = XB_T[0][1] * w_sub[1][0]
    v = v1 + v2
    reveal_all(v1, "v1")
    reveal_all(v2, "v2")
    reveal_all(v, "v")
    w_1 = matmul(XB_T, w_sub)
    reveal_all(w_1, "w_1")
    w_2 = mat_const_mul(alpha_B, w_1)
    w_res = matsub(w, w_2)
    mat_assign(w, w_res)

reveal_all(w, "w_final")

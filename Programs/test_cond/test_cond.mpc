# Params.set_params(int_precision=32,f=22,k=32)

# Test standard if-else
z = c_int(100)
if c_int(1) < c_int(2):
    z = c_int(200)


x = c_int(-1)
if c_int(1) < c_int(3):
    x = c_int(1)
else:
    x = c_int(2)

test(x, c_int(1))


y = c_int(10)
if c_int(-1) >= c_int(1):
    y = c_int(20)
else:
    y = c_int(30)


a = c_int(100)
if c_int(1) >= c_int(5):
    a = c_int(100)



# Test if-elif-else

b = c_int(25)

if c_int(0) < c_int(1):
    b = c_int(3)
elif c_int(0) < c_int(2):
    b = c_int(5)
else:
    b = c_int(10)


c = c_int(10)
if c_int(0) > c_int(1):
    c = c_int(21)
elif c_int(0) < c_int(3):
    c = c_int(31)
else:
    c = c_int(41)

d = c_int(1)
if c_int(0) <= c_int(1):
    d = c_int(22)
elif c_int(0) > c_int(3):
    d = c_int(38)
else:
    d = c_int(46)

e = c_int(3)
if c_int(0) <= c_int(1):
    e = c_int(5)
elif c_int(1) <= c_int(2):
    e = c_int(8)

f = c_int(10)
if c_int(1) > c_int(2):
    f = c_int(12)
elif c_int(3) < c_int(0):
    f = c_int(15)

test(z, c_int(200))
test(y, c_int(30))
test(a, c_int(100))
test(b, c_int(3))
test(c, c_int(31))
test(d, c_int(22))
test(e, c_int(5))
test(f, c_int(10))
